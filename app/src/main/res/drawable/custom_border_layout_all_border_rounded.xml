<?xml version="1.0" encoding="utf-8"?>
<layer-list
    xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape android:shape="rectangle">
            <solid android:color="@color/alto_d8d8d8" />
            <corners android:radius="@dimen/custom_border_rounded_radius" />
        </shape>
    </item>
    <item android:left="1dp" android:top="1dp" android:right="1dp" android:bottom="1dp">
        <shape android:shape="rectangle">
            <solid android:color="@color/white_solid_ffffff" />
            <corners android:radius="@dimen/custom_border_rounded_radius" />
        </shape>
    </item>
</layer-list>